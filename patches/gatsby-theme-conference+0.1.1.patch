+          <Styled.img
+            src={speaker.image}
+          />
+        </div>
+        <div
+          sx={{
+            flex: 'none',
+            p: 3,
+            width: [ '100%', '50%' ],
+          }}>
+          <Styled.p
+            sx={{
+              mb: 2
+            }}>
+            {speaker.bio}
+          </Styled.p>
+        </div>
+      </Flex>
+      </Container>
+    </Layout>
+  )
+}
+
+export const query = graphql`
+query($id: String!) {
+  speakersYaml(id: { eq: $id }) {
+    id
+    name
